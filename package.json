{
  "name": "golden-compass-club-backend",
  "version": "1.0.0",
  "description": "Backend API untuk Golden Compass Club",
  "main": "app.js",
  "type": "commonjs",
 "scripts": {
  "start": "node app.js",
  "dev": "nodemon app.js",
  "migrate": "sequelize-cli db:migrate --config config/config.js",
  "migrate:undo": "sequelize-cli db:migrate:undo --config config/config.js",
  "migrate:undo:all": "sequelize-cli db:migrate:undo:all --config config/config.js",
  "migrate:status": "sequelize-cli db:migrate:status --config config/config.js"
},

  "keywords": [
    "express",
    "sequelize",
    "postgresql",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
